{
  "name": "worldvibe",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:safe": "node scripts/launch.js",
    "dev:seed": "node scripts/launch.js --seed",
    "dev:mock": "node scripts/start-with-mock-db.js",
    "dev:sqlite": "node sqlite-setup.js && next dev",
    "build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.config.js --testPathPattern=tests/integration",
    "test:e2e": "cypress run",
    "cypress:open": "cypress open",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:validate": "prisma validate",
    "db:push": "prisma db push",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:setup": "node scripts/setup-database.js",
    "db:setup:seed": "node scripts/setup-database.js --seed",
    "db:test": "node scripts/test-database.js",
    "db:create:migration": "node scripts/db-migrate.js create",
    "db:apply:migration": "node scripts/db-migrate.js deploy",
    "db:status": "node scripts/db-migrate.js status",
    "db:reset": "node scripts/db-migrate.js reset",
    "db:seed": "node scripts/db-migrate.js seed",
    "db:check": "node scripts/check-database.js",
    "db:verify": "node scripts/verify-db.js",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "postinstall": "prisma generate",
    "analyze": "ANALYZE=true next build",
    "production:build": "npm run build",
    "production:start": "npm run start",
    "predeploy": "node scripts/pre-deploy-check.js",
    "deploy": "npm run build && npm run start",
    "validate": "npm-run-all --parallel type-check lint test build"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.13.8",
    "@aws-sdk/client-kinesis": "3.303.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fingerprintjs/fingerprintjs": "^4.6.0",
    "@hookform/resolvers": "^4.1.0",
    "@mui/material": "^6.4.5",
    "@nivo/bar": "^0.88.0",
    "@nivo/core": "^0.88.0",
    "@nivo/geo": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/pie": "^0.88.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^1.30.1",
    "@opentelemetry/exporter-prometheus": "^0.57.1",
    "@opentelemetry/sdk-metrics": "^1.30.1",
    "@opentelemetry/sdk-metrics-base": "^0.31.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@sentry/nextjs": "^7.80.0",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-virtual": "^3.13.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.19.2",
    "@vercel/analytics": "^1.4.1",
    "@vercel/kv": "^3.0.0",
    "@vercel/speed-insights": "^1.1.0",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.1",
    "classnames": "2.3.2",
    "clsx": "^1.2.1",
    "date-fns": "2.29.3",
    "dotenv": "^16.4.1",
    "framer-motion": "^12.4.1",
    "geist": "^1.3.1",
    "ioredis": "^5.5.0",
    "kafkajs": "^2.2.4",
    "lucide-react": "^0.279.0",
    "next": "14.0.3",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.4",
    "notistack": "^3.0.2",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.7.0",
    "postcss": "^8.4.21",
    "prisma": "^5.10.0",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.43.9",
    "react-intersection-observer": "^9.15.1",
    "recharts": "^2.6.2",
    "redis": "^4.6.7",
    "sonner": "^1.7.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^3.2.7",
    "ws": "8.13.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "cypress": "12.4.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "29.4.3",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prisma-dbml-generator": "^0.12.0",
    "ts-jest": "^29.1.2",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}

