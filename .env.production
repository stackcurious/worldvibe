# Supabase PostgreSQL Configuration
# Get these from your Supabase project settings -> Database
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres"
DIRECT_DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.[YOUR-PROJECT-REF].supabase.co:5432/postgres"

# Optional: Upstash Redis for caching (recommended for production)
# Get from https://console.upstash.com/
UPSTASH_REDIS_REST_URL=""
UPSTASH_REDIS_REST_TOKEN=""

# WebSocket URL (set to your production domain)
NEXT_PUBLIC_WS_URL="https://your-domain.com"

# Environment
NODE_ENV="production"

# Logging
LOG_QUERIES="false"

# Feature Flags
DB_MOCK="false"
